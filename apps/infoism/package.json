{
  "name": "infoism",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "pidanmeng",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run build:web && pnpm run build:win",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-builder --win --config",
    "build:mac": "electron-builder --mac --config",
    "build:linux": "electron-builder --linux --config",
    "build:web": "pnpm run typecheck && electron-vite build",
    "release": "npx semantic-release"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@infoism/lib": "latest",
    "@vueuse/core": "^9.10.0",
    "@vueuse/shared": "^9.10.0",
    "electron-updater": "^5.3.0",
    "i18next": "^22.4.8",
    "i18next-vue": "^2.1.1",
    "pinia": "^2.0.28",
    "qiankun": "^2.8.4",
    "unocss": "^0.48.0",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.42.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@iconify-json/carbon": "^1.1.13",
    "@iconify-json/clarity": "^1.1.7",
    "@iconify-json/mdi": "^1.1.45",
    "@iconify-json/ri": "^1.1.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.11.22",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "less": "^4.1.3",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^3.2.4",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.9"
  }
}
